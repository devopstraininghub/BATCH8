// structure concept
#include <stdio.h>
#include<string.h>
struct student {
    char name[20];
    int roll;
    float cgpa;
};

int main() 
{
    struct student s1;
    s1.roll = 1;
    s1.cgpa = 8.5;
    strcpy(s1.name, "anand" );
    printf("student name= %s\n", s1.name);
    printf("student roll no %d\n", s1.roll);
    printf("cgpa %f\n",s1.cgpa);
    
    struct student s2;
    s2.roll = 2;
    s2.cgpa =9.5;
    strcpy(s2.name, "pardhu");
    printf("student name= %s\n",s2.name);
    printf("student roll no %d\n", s2.roll);
    printf("cgpa %f\n", s2.cgpa);
    
    

    return 0;
}

